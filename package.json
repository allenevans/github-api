{
  "name": "github-api-action",
  "version": "0.0.1",
  "private": true,
  "description": "Github api action wrapper",
  "main": "dist/github-api.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "clean": "rimraf dist/",
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "prebuild": "npm run clean",
    "release": "semantic-release",
    "test": "jest --config=jest.config.js --silent --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allenevans/github-api.git"
  },
  "keywords": [
